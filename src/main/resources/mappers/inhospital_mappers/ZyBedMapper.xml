<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.gson.hisspring.model.mapper.inhospital_module_mapper.zyBedMapper">

    <resultMap id="zyBedMap" type="cn.gson.hisspring.model.pojos.ZyBed">
        <result column="bd_id" property="bdId"/>
        <result column="bd_name" property="bdName" />
    </resultMap>

    <select resultMap="zyBedMap" id="selectAllZyBed">
        select * from zy_bed
    </select>

</mapper>